{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsxImportSource": "hono/jsx",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022",
      "ESNext"
    ],
    "types": [
      "node",
      "vite/client"
    ],
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./app/*"
      ]
    },
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "**/*",
    "**/.server/**/*",
    "**/.client/**/*",
    ".react-router/types/**/*"
  ]
}